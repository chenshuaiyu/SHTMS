                    <font>
                        <Font size="14.0"/>
                    </font>
                </Label>
                <Label fx:id="mUserNoticePublic" alignment="CENTER" layoutX="0.0" layoutY="222.0"
                       onMouseClicked="#mUserNoticePublicClicked" prefHeight="36.0" prefWidth="158.0" text="用户公告发布"
