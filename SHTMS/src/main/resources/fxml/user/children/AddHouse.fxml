<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.controller.User.children.AddHouseController">
    <children>
        <Label layoutX="40.0" layoutY="30.0" text="省份" />
        <Label layoutX="235.0" layoutY="30.0" text="市" />
        <Label layoutX="430.0" layoutY="30.0" text="区/县" />

        <Label layoutX="40.0" layoutY="100.0" text="小区" />
        <Label layoutX="235.0" layoutY="100.0" text="楼盘编号" />
        <Label layoutX="430.0" layoutY="100.0" text="层" />

        <Label layoutX="40.0" layoutY="170.0" text="门牌号" />
        <Label layoutX="235.0" layoutY="170.0" text="面积(㎡)" />
        <Label layoutX="430.0" layoutY="170.0" text="价格(万)" />

        <Label layoutX="40.0" layoutY="240.0" text="物业费(元)" />
        <Label layoutX="235.0" layoutY="240.0" text="装修情况" />
        <Label layoutX="430.0" layoutY="240" text="车库" />

        <Label layoutX="40.0" layoutY="310.0" text="室" />
        <Label layoutX="235.0" layoutY="310.0" text="厅" />

        <JFXTextField fx:id="mProvinceTextField" layoutX="40.0" layoutY="50.0" />
        <JFXTextField fx:id="mCityTextField" layoutX="235.0" layoutY="50.0" />
        <JFXTextField fx:id="mCountyTextField" layoutX="430.0" layoutY="50.0" />

        <JFXTextField fx:id="mCommunityTextField" layoutX="40.0" layoutY="120.0" />
        <JFXTextField fx:id="mBuildingNoTextField" layoutX="235.0" layoutY="120.0" />
        <JFXTextField fx:id="mFloorTextField" layoutX="430.0" layoutY="120.0" />

        <JFXTextField fx:id="mNoTextField" layoutX="40.0" layoutY="190.0" />
        <JFXTextField fx:id="mAreaTextField" layoutX="235.0" layoutY="190.0" />
        <JFXTextField fx:id="mPriceTextField" layoutX="430.0" layoutY="190.0" />

        <JFXTextField fx:id="mPropertyCostTextField" layoutX="40.0" layoutY="260.0" />
        <ChoiceBox fx:id="mIsDecoratedTextField" layoutX="235.0" layoutY="260.0" prefWidth="150.0" />
        <ChoiceBox fx:id="mHasGarageTextField" layoutX="430.0" layoutY="260.0" prefWidth="150.0" />

        <JFXTextField fx:id="mRoomTextField" layoutX="40.0" layoutY="330.0" />
        <JFXTextField fx:id="mLivingRoomTextField" layoutX="235.0" layoutY="330.0" />


        <Button fx:id="mAddButton" layoutX="426.0" layoutY="344.0" mnemonicParsing="false" onMouseClicked="#mAddButtonClicked" prefHeight="32.0" prefWidth="68.0" style="-fx-background-color: #4f75ff;" text="新  增">
            <font>
                <Font size="14.0" />
            </font>
        </Button>
        <Button fx:id="mCancelButton" layoutX="518.0" layoutY="344.0" mnemonicParsing="false" onMouseClicked="#mCancelButtonClicked" prefHeight="32.0" prefWidth="68.0" style="-fx-background-color: #4f75ff;" text="取  消">
            <font>
                <Font size="14.0" />
            </font>
        </Button>

    </children>
</AnchorPane>
