        <JFXTextField fx:id="mEmail" layoutX="34.0" layoutY="225.0"/>
        <Button fx:id="mEditButton" layoutX="266.0" layoutY="403.0" mnemonicParsing="false"
                onMouseClicked="#mEditButtonClicked" prefHeight="36.0" prefWidth="94.0" text="编   辑"/>
        <Button fx:id="mCompleteButton" layoutX="186.0" layoutY="403.0" mnemonicParsing="false"
                onMouseClicked="#mCompleteButtonClicked" prefHeight="36.0" prefWidth="94.0" text="完   成"
                visible="false"/>
        <Button fx:id="mCancelButton" layoutX="346.0" layoutY="403.0" mnemonicParsing="false"
                onMouseClicked="#mCancelButtonClicked" prefHeight="36.0" prefWidth="94.0" text="取   消"
                visible="false"/>

    </children>
</AnchorPane>
