<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="665.0" xmlns="http://javafx.com/javafx/8.0.121"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.controller.User.UPersonalInfomationController">
    <children>
        <Label layoutX="34.0" layoutY="36.0" text="用户名" textFill="#047aa1"/>
        <Label layoutX="252.0" layoutY="36.0" text="真实姓名" textFill="#047aa1"/>
        <Label layoutX="470.0" layoutY="36.0" text="身份证号" textFill="#047aa1"/>
        <Label layoutX="33.0" layoutY="116.0" text="性别" textFill="#047aa1"/>
        <Label layoutX="251.0" layoutY="116.0" text="年龄" textFill="#047aa1"/>
        <Label layoutX="469.0" layoutY="116.0" text="联系方式" textFill="#047aa1"/>
        <Label layoutX="30.0" layoutY="205.0" text="Email" textFill="#047aa1"/>
        <JFXTextField fx:id="mUsername" layoutX="34.0" layoutY="56.0" unFocusColor="BLACK"/>
        <JFXTextField fx:id="mName" layoutX="252.0" layoutY="56.0"/>
        <JFXTextField fx:id="mIdentityNumber" layoutX="470.0" layoutY="56.0"/>
        <ChoiceBox fx:id="mSexChoiceBox" layoutX="33.0" layoutY="146.0" prefWidth="150.0"/>
        <ChoiceBox fx:id="mAgeChoiceBox" layoutX="251.0" layoutY="146.0" prefWidth="150.0"/>
        <JFXTextField fx:id="mSexTextField" layoutX="33.0" layoutY="146.0" prefWidth="150.0"/>
