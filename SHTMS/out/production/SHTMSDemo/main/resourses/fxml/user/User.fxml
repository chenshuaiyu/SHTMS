<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="665.0" xmlns="http://javafx.com/javafx/8.0.121"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.controller.User.UserController">
    <children>
        <AnchorPane prefHeight="498.0" prefWidth="158.0" style="-fx-background-color: #647efc;">
            <children>
                <Label fx:id="mSellHouseManager" alignment="CENTER" contentDisplay="CENTER" layoutX="0.0"
                       layoutY="168.0" onMouseClicked="#mSellHouseManagerClicked" prefHeight="36.0" prefWidth="158.0"
                       text="出售房源管理" textFill="WHITE">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </Label>
                <Label fx:id="mHouseSellProgress" alignment="CENTER" layoutX="0.0" layoutY="204.0"
                       onMouseClicked="#mHouseSellProgressClicked" prefHeight="36.0" prefWidth="158.0" text="房源出售进度"
                       textFill="WHITE">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </Label>
                <Label fx:id="mQueryTargetHouse" alignment="CENTER" layoutX="0.0" layoutY="240.0"
                       onMouseClicked="#mQueryTargetHouseClicked" prefHeight="36.0" prefWidth="158.0" text="查找目标房源"
                       textFill="WHITE">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </Label>
                <Label fx:id="mMyFavoriteHouses" alignment="CENTER" layoutX="0.0" layoutY="276.0"
                       onMouseClicked="#mMyFavoriteHousesClicked" prefHeight="36.0" prefWidth="158.0" text="我的心仪房源"
                       textFill="WHITE">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </Label>
                <Label fx:id="mPersonalInformationManager" alignment="CENTER" layoutX="0.0" layoutY="312.0"
                       onMouseClicked="#mPersonalInformationManagerClicked" prefHeight="36.0" prefWidth="158.0"
                       text="个人信息管理" textFill="WHITE">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </Label>
                <Button fx:id="mExitButton" layoutX="14.0" layoutY="443.0" mnemonicParsing="false"
                        onAction="#mExitButtonClicked" prefHeight="35.0" prefWidth="130.0" text="退   出"
                        textFill="WHITE">
                    <font>
                        <Font size="16.0"/>
                    </font>
                </Button>
                <ImageView fitHeight="100.0" fitWidth="100.0" layoutX="28.0" layoutY="16.0" pickOnBounds="true"
                           preserveRatio="true">
                    <image>
                        <Image url="@../../assets/icon.png"/>
                    </image>
                </ImageView>
                <Label alignment="CENTER" contentDisplay="CENTER" layoutX="0.0" layoutY="115.0" prefHeight="25.0"
                       prefWidth="158.0" text="username">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </Label>
                <Label layoutX="12.0" layoutY="384.0" prefHeight="35.0" prefWidth="74.0" text="当前角色：" textFill="WHITE">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </Label>
                <Label layoutX="79.0" layoutY="384.0" prefHeight="35.0" prefWidth="74.0" text="普通用户">
                    <font>
                        <Font size="16.0"/>
                    </font>
                </Label>

            </children>
        </AnchorPane>
        <AnchorPane fx:id="mContainer" layoutX="158.0" prefHeight="500.0" prefWidth="665.0"/>
    </children>
    <stylesheets>
        <URL value="@../../css/user.css"/>
    </stylesheets>
</AnchorPane>
