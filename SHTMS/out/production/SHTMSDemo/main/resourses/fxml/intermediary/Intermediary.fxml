<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Label?>
<?import java.net.URL?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="main.java.controller.intermediary.IntermediaryController"
            prefHeight="500.0" prefWidth="665.0">
    <children>
        <AnchorPane prefHeight="498.0" prefWidth="158.0" style="-fx-background-color: #647efc;">
            <children>
                <Label fx:id="mManagerHouseBuy" alignment="CENTER" contentDisplay="CENTER" layoutX="0.0"
                       layoutY="168.0" onMouseClicked="#mManagerHouseBuyClicked" prefHeight="36.0"
                       prefWidth="158.0"
                       text="管理房源购买" textFill="WHITE">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </Label>
                <Label fx:id="mManagerTransactionInformation" alignment="CENTER" layoutX="0.0" layoutY="204.0"
                       onMouseClicked="#mManagerTransactionInformationClicked" prefHeight="36.0" prefWidth="158.0"
                       text="管理交易信息"
                       textFill="WHITE">
                    <font>
                        <Font size="14.0"/>

                    </font>
                </Label>
                <Label fx:id="mPersonalInformationManager" alignment="CENTER" layoutX="0.0" layoutY="240.0"
                       onMouseClicked="#mPersonalInformationManagerClicked" prefHeight="36.0" prefWidth="158.0"
                       text="个人信息管理"
                       textFill="WHITE">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </Label>
                <Button fx:id="mExitButton" layoutX="14.0" layoutY="407.0" mnemonicParsing="false" prefHeight="35.0"
                        prefWidth="130.0" text="退   出" textFill="WHITE" onAction="#mExitButtonClicked">
                    <font>
                        <Font size="16.0"/>
                    </font>
                </Button>
                <ImageView fitHeight="100.0" fitWidth="100.0" layoutX="28.0" layoutY="16.0" pickOnBounds="true"
                           preserveRatio="true">
                    <image>
                        <Image url="@../../assets/icon.png"/>
                    </image>
                </ImageView>
                <Label alignment="CENTER" contentDisplay="CENTER" layoutX="0.0" layoutY="115.0" prefHeight="25.0"
                       prefWidth="158.0" text="intermediaryname">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </Label>
                <Label layoutX="12.0" layoutY="348.0" prefHeight="35.0" prefWidth="74.0" text="当前角色：" textFill="WHITE">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </Label>
                <Label layoutX="79.0" layoutY="348.0" prefHeight="35.0" prefWidth="74.0" text="中介人员">
                    <font>
                        <Font size="16.0"/>
                    </font>
                </Label>

            </children>
        </AnchorPane>
        <AnchorPane fx:id="mContainer" layoutX="158.0" prefHeight="500.0" prefWidth="665.0"/>
    </children>
    <stylesheets>
        <URL value="@../../css/intermediary.css"/>
    </stylesheets>
</AnchorPane>
